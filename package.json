{
  "name": "node-knockout-react-node-starter",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.15.4",
    "express-enforces-ssl": "^1.1.0",
    "express-joi-validator": "^2.0.0",
    "helmet": "^3.8.1",
    "joi": "^13.0.1",
    "node-sass-chokidar": "0.0.3",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "sequelize": "^4.22.6",
    "sequelize-cli": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"node index\" \"npm run watch-css\"",
    "heroku-postbuild": "react-scripts build",
    "build": "concurrently \"react-scripts build\" \"npm run build-css\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "lint": "eslint **\\*.js",
    "test-api": "mocha ./api-tests/**.spec.js",
    "test-api-watch": "mocha ./api-tests/**.spec.js --watch"
  },
  "engines": {
    "node": "8.9.1",
    "yarn": "1.3.2",
    "npm": "5.5.1"
  },
  "proxy": "http://localhost:3001"
}
